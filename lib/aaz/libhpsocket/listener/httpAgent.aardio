namespace aaz.libhpsocket.listener;
var _dll = ..aaz.libhpsocket._dll;

class httpAgent{
	ctor(){
		this = ..aaz.libhpsocket.listener();
		this.pListener = _dll.Create_HP_HttpAgentListenerP(); 
		this.componentClassPath = "component.httpAgent";
	};	
	destroy = function(){
		if(this.pListener){
